{
  "editor.codeActionsOnSave": {
    "source.fixAll.pylance": "explicit",
    "source.unusedImports": "explicit",
    "source.fixAll.ruff": "explicit",
    "source.organizeImports.ruff": "explicit"
  },
  "files.associations": {
    "**/components/**/*.html": "django-html"
  },
  "emmet.includeLanguages": {
    "django-html": "html"
  }
}
