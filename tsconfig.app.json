{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "allowImportingTsExtensions": true,
    "emitDeclarationOnly": true,
    "noEmit": false
  },
  "include": [
    "env.d.ts",
    "static/src/**/*",
    "static/src/**/*.vue"
  ],
  "exclude": [
    "static/src/**/__tests__/*",
    "./.venv"
  ]
}
